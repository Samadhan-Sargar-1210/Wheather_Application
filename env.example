# Weather App Environment Variables
# Copy this file to .env and fill in your actual values

# OpenWeatherMap API Key (Required)
# Get your free API key from: https://openweathermap.org/api
VITE_OPENWEATHER_API_KEY=your_openweathermap_api_key_here

# Optional: Override API base URL (for testing or custom endpoints)
# VITE_API_BASE_URL=https://api.openweathermap.org

# Optional: Enable debug mode
# VITE_DEBUG_MODE=false

# Optional: Set default language
# VITE_DEFAULT_LANGUAGE=en

# Optional: Enable analytics (if you add analytics later)
# VITE_ANALYTICS_ID=your_analytics_id_here 